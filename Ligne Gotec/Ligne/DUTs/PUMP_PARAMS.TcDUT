<?xml version="1.0" encoding="utf-8"?>
<TcPlcObject Version="1.1.0.1" ProductVersion="3.1.0.26">
  <DUT Name="PUMP_PARAMS" Id="{dcfcedb9-d4fd-45ca-b68e-7214757f8e01}">
    <Declaration><![CDATA[TYPE PUMP_PARAMS :
STRUCT
	
(* Général *******************************************************************************************************************)

(* Station 1 Haute tension ***************************************************************************************************)
	ST1_HT_NO_PROG					:INT:=1;			(*Numéro de programme de l'appareil de test HT*)

(* Station 2 aspiration ******************************************************************************************************)
	ST2_CONTRE_PRESSION						:BOOL:=0;			(*Contre pression d'air TRUE: avec contre pression FALSE: sans contre pression*)
	ST2_TEMPS_POMPAGE						:TIME:= T#10S;		(*Temps d'amorcage *)
	ST2_TEMPS_MESURE_ASPIRATION				:TIME:= T#5S;		(*Temps de mesure aspiration*)
	ST2_TEMPS_MESURE_ETANCHEITE_SOUPAPES	:TIME:= T#5S;		(*Temps de mesure étachéité des soupapes*)
		
(* Station 3 étanchéité ******************************************************************************************************)

(* Station 4 Débit petit *****************************************************************************************************)

(* Station 5 Débit Grands*****************************************************************************************************)

(* Station 6 Pmax ************************************************************************************************************)	
	ST6_TEMPS_POMPAGE				:TIME:= T#10S;		(*Temps amorcage de la pompte *)
	ST6_TEMPS_MESURE				:TIME:= T#5S;		(*Temps de mesure de Pmax *)
	
(* Station 7 Sechage *********************************************************************************************************)
	ST7_TEMPS_SECHAGE				:TIME:= T#2S;		(*Temps séchage pompe station 7 *)

	

	
	(*Q1_ENABLE:BOOL				:=TRUE;				(*Enable Test*)
	Q1_PUMPING_TIME:TIME			:=T#20s; 			(*Pumping delay to prepare the pump for the flow rate test *)
	Q1_MEASUREMENT_TIME:TIME		:=T#10s; 			(*Measurement time for the flow rate*)
	Q1_FLOW_TARGET:REAL				:=350; 				(*Flow rate target [ml/min]*)
	Q1_FLOW_LIMIT_UP:REAL			:=800; 				(*Flow rate Uper limit. Q>This limit -> NO GOOD [ml/min]*)
	Q1_FLOW_LIMIT_DOWN:REAL			:=100; 				(*Flow rate Lower limit. Q<This limit -> NO GOOD [ml/min]*)
	Q1_PRESSURE_TARGET:REAL			:=8; 				(*Pressure target [bar]*)
	Q1_PRESSURE_LIMIT_UP:REAL		:=15; 				(*Pressure Uper limit. p>This limit -> NO GOOD [bar]*)
	Q1_PRESSURE_LIMIT_DOWN:REAL		:=4;	 			(*Pressure Lower limit. p<This limit -> NO GOOD [bar]*)
	Q1_MAX_FLOW:BOOL				:=FALSE;			(*Use the max flow bypass*)*)


END_STRUCT
END_TYPE]]></Declaration>
    <ObjectProperties />
  </DUT>
</TcPlcObject>